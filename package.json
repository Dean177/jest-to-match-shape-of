{
  "contributors": [
    {
      "name": "skmdev",
      "email": "skmdev29@gmail.com",
      "url": "https://github.com/skmdev"
    }
  ],
  "dependencies": {
    "jest-get-type": "22.4.3",
    "jest-matcher-utils": "23.6.0",
    "lodash": "^4.17.11"
  },
  "description": "A jest matcher to verify the structure of an object, particularly useful for api integration tests",
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/jest": "23.3.10",
    "@types/jest-get-type": "21.0.0",
    "@types/jest-matcher-utils": "21.0.1",
    "@types/lodash": "4.14.118",
    "codecov": "3.1.0",
    "jest": "23.6.0",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/common-types.ts",
      "<rootDir>/src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "name": "jest-to-match-shape-of",
  "peerDependencies": {
    "jest": "21.x"
  },
  "repository": "git@github.com:Dean177/jest-to-match-shape-of.git",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --noEmit --pretty --watch",
    "lint": "tslint ./src/**/*.ts",
    "prepublish": "yarn build && yarn lint && yarn test",
    "report-coverage": "codecov",
    "test": "jest"
  },
  "version": "1.2.0"
}
